{
  "name": "@hubot-friends/hubot-chat",
  "version": "0.1.0",
  "description": "A private, ephemeral chat app where Hubot is native",
  "type": "module",
  "main": "./src/index.mjs",
  "exports": {
    ".": "./src/index.mjs"
  },
  "scripts": {
